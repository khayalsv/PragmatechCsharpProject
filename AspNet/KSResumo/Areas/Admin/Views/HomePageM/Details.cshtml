@model Home
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<table class="table">
    <thead>
        <tr>
            <th>Image</th>
            <th>Text</th>
            <th>Title</th>
            <th>HolderTitle</th>
            <th>Tag1</th>
            <th>Tag2</th>
            <th>Tag3</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">
                <img width="200" src="~/img/@Model.Image" />
            </th>
            <td>@Model.Text</td>
            <td>@Model.Title</td>
            <td>@Model.HolderTitle</td>
            <td>@Model.Tag1</td>
            <td>@Model.Tag2</td>
            <td>@Model.Tag3</td>
        </tr>
    </tbody>
</table>

