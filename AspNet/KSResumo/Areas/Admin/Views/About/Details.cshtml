@model About
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<table class="table">
    <thead>
        <tr>
            <th>Image</th>
            <th>Text</th>
            <th>Name</th>
            <th>Birthday</th>
            <th>Age</th>
            <th>Address</th>
            <th>Phone</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">
                <img width="200" src="~/img/@Model.Image" />
            </th>
            <td>@Model.Text</td>
            <td>@Model.Name</td>
            <td>@Model.Birthday</td>
            <td>@Model.Age</td>
            <td>@Model.Address</td>
            <td>@Model.Phone</td>
            <td>@Model.Email</td>
        </tr>
    </tbody>
</table>

