@inject Microsoft.AspNetCore.Identity.UserManager<IdentityUser> userManager;

@{
    IdentityUser user = null;
    if (User.Identity.IsAuthenticated)
    {
        user = userManager.Users.FirstOrDefault(x => x.UserName == User.Identity.Name);
    }
}


