@model HomeVM
<!-- Wrapper All -->
<div class="resumo_fn_wrapper">

    <!-- MODALBOX -->
   
    <!-- /MODALBOX -->

    <div class="resumo_fn_content">

        <!-- Main Left Part -->
        <div class="resumo_fn_left">

            <!-- Page -->
            <div class="resumo_fn_page">


                <!-- Home Section -->
                <section id="home">
                    <div class="container">
                        <div class="roww">

                            <!-- Main Title -->
                            @foreach (var item in Model.Homes)
                            {<div class="resumo_fn_main_title">
                                    <h3 class="subtitle">Introduction</h3>
                                    <h3 class="title">@item.Title</h3>
                                    <p class="desc">@item.Text</p>
                                </div>
                            }
                            <!-- /Main Title -->

                        </div>
                    </div>
                </section>
                <!-- /Home Section -->
                <!-- About Section -->
                @foreach (var item in Model.Abouts)
                {
                    <section id="about">
                        <div class="container">
                            <div class="roww">

                                <!-- Main Title -->
                                <div class="resumo_fn_main_title">
                                    <h3 class="subtitle">About Me</h3>
                                    <h3 class="title">Biography</h3>
                                    <p class="desc">@item.Text</p>
                                </div>
                                <!-- /Main Title -->
                                <!-- About Information -->
                                <div class="resumo_fn_about_info">
                                    <div class="about_left">
                                        <table>
                                            <tr>
                                                <th>Name</th>
                                                <th>@item.Name</th>
                                            </tr>
                                            <tr>
                                                <th>Birthday</th>
                                                <th>@item.Birthday</th>
                                            </tr>
                                            <tr>
                                                <th>Age</th>
                                                <th>@item.Age</th>
                                            </tr>
                                            <tr>
                                                <th>Address</th>
                                                <th>@item.Address</th>
                                            </tr>
                                            <tr>
                                                <th>Phone</th>
                                                <th>@item.Phone</th>
                                            </tr>
                                            <tr>
                                                <th>Email</th>
                                                <th>@item.Email</th>
                                            </tr>
                                        </table>
                                    </div>
                                    <div class="about_right">
                                        <!-- Download CV Button -->
                                        <div class="resumo_fn_cv_btn">
                                            <a href="img/@item.Image" download>
                                                <span class="icon">
                                                    <img src="svg/inbox.svg" alt="" class="fn__svg" />
                                                    <img src="svg/arrow.svg" alt="" class="fn__svg arrow" />
                                                </span>
                                                <span>Download CV</span>
                                            </a>
                                        </div>
                                        <!-- /Download CV Button -->
                                    </div>
                                </div>
                                <!-- /About Information -->

                            </div>
                        </div>
                    </section>

                }                <!-- /About Section -->
                <!-- Portfolio Section -->
                <section id="portfolio">

                    <div class="container">
                        <div class="roww">
                            <!-- Main Title -->
                            <div class="resumo_fn_main_title">
                                <div class="my__nav">
                                    <a href="#" class="prev"><span></span></a>
                                    <a href="#" class="next"><span></span></a>
                                </div>
                                <h3 class="subtitle">Portfolio</h3>
                                <h3 class="title">Featured Projects</h3>
                            </div>
                            <!-- /Main Title -->
                        </div>
                    </div>

                    <div class="container noright">
                        <div class="row">

                            <div class="owl-carousel modal_items" data-from="portfolio" data-count="5">

                                    @foreach (var item in Model.Portfolios)
                                    {

                                        <div class="item MmodalL_item" data-index="@item.ID">
                                            <div class="img_holder">
                                                <img src="img/thumb/square.jpg" alt="">
                                                <div class="abs_img" data-bg-img="img/@item.Image"></div>
                                            </div>
                                            <div class="title_holder">
                                                <h3><a href="@item.Link" target="_blank">@item.Name</a></h3>
                                            </div>
                                        </div>
                                    }

                            </div>

                        </div>
                    </div>

                </section>
