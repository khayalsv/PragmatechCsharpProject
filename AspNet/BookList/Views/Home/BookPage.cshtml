<h3>Library</h3>

<div class="container">
    <div class="row">

        @foreach (var item in ViewBag.imgBOX)
        {
        <div class="col-md-4">
            <div class="card" style="width:300px; height:600px">
                <img src="~/@item.Link" class="card-img-top" alt="..." height="400">
                <div class="card-body">
                    <h5 class="card-title">@item.Name</h5>
                </div>
            </div>
        </div>
        }
    </div>
</div>