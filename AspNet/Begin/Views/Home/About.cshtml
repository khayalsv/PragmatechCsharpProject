<h3>Images</h3>



<div class="container">
    <div class="row">

       @foreach (var item in ViewBag.imgBOX)
       {
        <div class="col-md-4">
            <div class="card" style="width:300px; height:300px">
                <img src="~/@item.Link" class="card-img-top" alt="..." height="200">
                <div class="card-body">
                    <h5 class="card-title">@item.Title</h5>
                </div>
            </div>
        </div>
       }


    </div>
</div>