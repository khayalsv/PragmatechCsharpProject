@using EntityLayer.Concrete
@model List<AppUser>
 
@{
    ViewData["Title"] = "UserRoleList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>UserRoleList</h1>

<table class="table table-bordered">
    <tr>
        <th>User</th>
        <th>Role Edit</th>
    </tr>
    @foreach (var item in Model)
	{
		 <tr>
             <td>@item.UserName</td>
             <td><a href="/Admin/AdminRole/AsSignRole/@item.Id/" class="btn btn-outline-warning">Role Edit</a></td>
         </tr>
	}
</table>
<a href="/Admin/AdminRole/Index" class="btn btn-primary">Go To Role List</a>
