@model Home

@{
    ViewData["Title"] = "Create";
}


<form method="post" enctype="multipart/form-data">
    @*<div asp-validation-summary="All"></div>*@

    <div class="mb-3">
        <label class="form-label">Title 1</label>
        <input class="form-control" type="text" asp-for="Title1" />
        <span asp-validation-for="Title1"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">Title 2</label>
        <input class="form-control" type="text" asp-for="Title2" />
        <span asp-validation-for="Title2"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">Video Link</label>
        <input class="form-control" type="text" asp-for="VideoLink" />
        <span asp-validation-for="VideoLink"></span>
    </div>


    <div class="mb-3">
        <label class="form-label">Photo</label>
        <input class="form-control" type="file" asp-for="Photo">
        <span asp-validation-for="Photo"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>

