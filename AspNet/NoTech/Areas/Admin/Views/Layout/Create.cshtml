@model Layout

@{
    ViewData["Title"] = "Create";
}

<form method="post" enctype="multipart/form-data">
    @*<div asp-validation-summary="All"></div>*@

    <div class="mb-3">
        <label class="form-label">Address</label>
        <input class="form-control" type="text" asp-for="Address" />
        <span asp-validation-for="Address"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">Email</label>
        <input class="form-control" type="text" asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">SocialLink1</label>
        <input class="form-control" type="text" asp-for="SocialLink1" />
        <span asp-validation-for="SocialLink1"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">SocialIcon1</label>
        <input class="form-control" type="text" asp-for="SocialIcon1" />
        <span asp-validation-for="SocialIcon1"></span>
    </div>




    <div class="mb-3">
        <label class="form-label">SocialLink2</label>
        <input class="form-control" type="text" asp-for="SocialLink2" />
        <span asp-validation-for="SocialLink2"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">SocialIcon2</label>
        <input class="form-control" type="text" asp-for="SocialIcon2" />
        <span asp-validation-for="SocialIcon2"></span>
    </div>




    <div class="mb-3">
        <label class="form-label">SocialLink3</label>
        <input class="form-control" type="text" asp-for="SocialLink3" />
        <span asp-validation-for="SocialLink3"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">SocialIcon3</label>
        <input class="form-control" type="text" asp-for="SocialIcon3" />
        <span asp-validation-for="SocialIcon3"></span>
    </div>



    <div class="mb-3">
        <label class="form-label">SocialLink4</label>
        <input class="form-control" type="text" asp-for="SocialLink4" />
        <span asp-validation-for="SocialLink4"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">SocialIcon4</label>
        <input class="form-control" type="text" asp-for="SocialIcon4" />
        <span asp-validation-for="SocialIcon4"></span>
    </div>
    
    
    <div class="mb-3">
        <label class="form-label">Question</label>
        <input class="form-control" type="text" asp-for="Question" />
        <span asp-validation-for="Question"></span>
    </div>

    <div class="mb-3">
        <label class="form-label">Phone</label>
        <input class="form-control" type="text" asp-for="Phone" />
        <span asp-validation-for="Phone"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>

