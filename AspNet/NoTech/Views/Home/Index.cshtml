                            </div>
                            <h3>@item.Odometer</h3>
                            <p class="counter-one__title">@item.Title</p>
                            <p class="counter-one__text">@item.Text</p>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</section>
