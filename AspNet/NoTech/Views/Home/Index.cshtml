        @foreach (var item in Model.helpings)
        {
            <div class="helping__left">
                <div class="helping__left-bg"
                     style="background-image: url(../assets/images/backgrounds/@item.Image);"></div>
            </div>
            <div class="helping__right">
                <div class="helping-shape-1  wow slideInRight" data-wow-delay="100ms" data-wow-duration="2500ms">
                    <img src="~/assets/images/shapes/helping-shape-1.png" alt="">
                </div>
                <div class="helping__right-content">
                    <div class="section-title text-left">
                        <div class="section-title__tagline-box">
                            <span class="section-title__tagline">Technology solution</span>
                            <div class="section-title-shape">
                                <img src="~/assets/images/shapes/section-title-shape.png" alt="">
                            </div>
                        </div>
                        <h2 class="section-title__title">@item.Title</h2>
                    </div>
                    <p class="helping__text">
                        @item.Text
                    </p>
                    <ul class="list-unstyled helping__points">
                        <li>
                            <div class="icon">
                                <span class="icon-data-protection"></span>
                            </div>
                            <h3 class="helping__title">@item.SubTitle</h3>
                            <p class="helping__text-2">@item.SubText</p>
                        </li>
                    </ul>
                </div>
            </div>
        }
    </div>
</section>
<!--Helping End-->
<!--Blog One Start-->
<section class="blog-one" id="blog">
    <div class="container">
        <div class="section-title text-center">
            <div class="section-title__tagline-box">
                <span class="section-title__tagline">What’s happening</span>
                <div class="section-title-shape">
                    <img src="assets/images/shapes/section-title-shape.png" alt="">
                </div>
            </div>
            @foreach (var item in Model.blogs)
            {
                <h2 class="section-title__title">@item.Title</h2>

            }
        </div>
        <div class="row">
            @foreach (var item in Model.blogs)
            {
                <div class="col-xl-4 col-lg-4 wow fadeInUp" data-wow-delay="100ms">
                    <!--Blog One single-->
                    <div class="blog-one__single">
                        <div class="blog-one__img">
                            <img src="assets/images/blog/@item.Image" style="height:250px" alt="">

                            <a href="@item.Link">
                                <span class="blog-one__plus"></span>
                            </a>
                        </div>
                        <div class="blog-one__content">
                            <ul class="list-unstyled blog-one__meta">
                                <li>
                                    <a href="@item.Link"><i class="far fa-clock"></i> @item.DateTime</a>
                                </li>
                                <li>
                                    <a href="@item.Link"><i class="far fa-user-circle"></i> @item.byFrom</a>
                                </li>
                            </ul>
                            <h3 class="blog-one__title">
                                <a href="@item.Link">@item.Name</a>
                            </h3>
                            <div class="blog-one__bottom">
                                <a href="@item.Link">Learn more<i class="fa fa-long-arrow-alt-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>

            }
        </div>
    </div>
</section>
<!--Blog One End-->



<!--Contact Page Start-->
<section  class="blog-one" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <h2 class="section-title__title" style="padding-bottom: 30px" >Contact</h2>
                <div class="contact-page__form">
                    <div asp-validation-summary="All"></div>
                    <form method="post" enctype="multipart/form-data">
                        <div class="row">
                            <div class="col-xl-6">
                                <div class="comment-form__input-box">
                                    <input type="text" required placeholder="Your name" asp-for="contact.Name">
                                    <span asp-validation-for="contact.Name"></span>
                                </div>
                            </div>
                            <div class="col-xl-6">
                                <div class="comment-form__input-box">
                                    <input type="email" required placeholder="Email address" asp-for="contact.Email">
                                    <span asp-validation-for="contact.Email"></span>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xl-12">
                                <div class="comment-form__input-box text-message-box">
                                    <textarea required placeholder="Write comment" asp-for="contact.Message"></textarea>
                                    <span asp-validation-for="contact.Message"></span>
                                </div>
                                <div class="comment-form__btn-box">
                                    <button type="submit" class="thm-btn comment-form__btn">Send a message</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--Contact Page End-->

