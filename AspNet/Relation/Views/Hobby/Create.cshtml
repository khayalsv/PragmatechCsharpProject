@model TeacherToHobby

<form method="post">

    <div class="mb-3">
        <label>Hobby</label>
        <input type="text" class="form-control" asp-for="TeacherTable.TeacherName">
        <span asp-validation-for="TeacherTable.TeacherName" style="color:red"></span>
    </div>


    <div class="mb-3">
        <label>Hobby</label>
        <input type="text" class="form-control" asp-for="HobbyTable.HobbyName">
        <span asp-validation-for="HobbyTable.HobbyName" style="color:red"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>

